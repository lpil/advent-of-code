import first
import gleam/expect

pub fn fuel_requirement_test() {
  12
  |> first.fuel_requirement
  |> expect.equal(_, 2)

  12
  |> first.fuel_requirement
  |> expect.equal(_, 2)

  1969
  |> first.fuel_requirement
  |> expect.equal(_, 654)

  100756
  |> first.fuel_requirement
  |> expect.equal(_, 33583)
}

pub fn parse_input_data_test() {
  "1\n2\n300"
  |> first.parse_input_data
  |> expect.equal(_, Ok([1, 2, 300]))
}

